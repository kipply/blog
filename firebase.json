{
  "hosting": {
    "public": "build",
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
      "redirects": [ {
    "source": "/date",
    "destination": "https://www.notion.so/kipply/date-kipply-b896d42e248a4516b1edca799afe2577",
    "type": 301
  }],
    "rewrites": [
      {
        "source": "/",
        "destination": "/blog/index.html"
      },
      {
        "source": "/resume",
        "destination": "/CarolChenResume.pdf"
      },
      {
        "source": "/date",
        "destination": "https://www.notion.so/kipply/date-kipply-b896d42e248a4516b1edca799afe2577"
      },
      {
        "source": "/bestaplaid",
        "destination": "/projects/bestaplaid.html"
      },
      {
        "source": "/copywrite",
        "destination": "/projects/copywrite.html"
      },
      {
        "source": "/testing",
        "destination": "/projects/testing.html"
      },
      {
        "source": "/blog/**",
        "destination": "/blog/404.html"
      },
      {
        "source": "/eng-calc",
        "destination": "/index.html"
      },

      {
        "source": "**",
        "destination": "/404.html"
      },
      {
        "source": "/blog",
        "destination": "/blog/index.html"
      }
    ]
  }
}
