{
  "name": "carolchen.me",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "rc-slider": "^8.6.1",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-images": "^0.5.19",
    "react-photo-gallery": "^6.2.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.0",
    "react-transition-group": "^1.2.1",
    "react-typist": "^2.0.4",
    "request": "^2.88.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o public/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o public/ --watch --recursive  --output-style compressed",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js && cd blog && hexo generate",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1"
  }
}
